    "koa-bodyparser": "^3.2.0",
    "koa-logger": "^2.0.1",
    "koa-router": "^7.0.1",
    "ms": "^1.0.0",
    "persistgraphql": "^0.3.1",
    "prop-types": "^15.5.3",
    "ramda": "^0.23.0",
